{
    "name": "Microsoft.PowerShell.Commands.Diagnostics",
    "version": "1.0.0-*",
    "buildOptions": {
        "nowarn": [ "CS1591" ],
        "xmlDoc": true,
        "keyFile": "../signing/visualstudiopublic.snk",
        "delaySign": true,
        "publicSign": false,
        "warningsAsErrors": true
    },

    "dependencies": {
        "System.Management.Automation": "1.0.0-*"
    },

    "frameworks": {
        "netstandard1.6": {
            "imports": [ "dnxcore50" ],
            "buildOptions": {
                "define": [ "CORECLR" ],
                "debugType": "portable",
                "compile": {
                    "excludeFiles": [
                        "GetEventSnapin.cs",

                        "gen/GetEventResources.cs"
                    ]
                }
            }
        },
        
        "net451": {
            "buildOptions": {
                "compile": {
                    "excludeFiles": [
                        "CoreCLR/Stubs.cs",
                        "gen/GetEventResources.cs"
                    ]
                }
            }
        }
    }
}
