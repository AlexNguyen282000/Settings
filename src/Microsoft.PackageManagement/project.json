                "System.Net.Http": "",
                "System.IO.Compression": "",
                "System.IO.Compression.FileSystem": ""
            }
        },
        "netstandard1.5": {
            "imports": [ "dnxcore50" ],
            "compilationOptions": {
                "define": [ "CORECLR" ]
