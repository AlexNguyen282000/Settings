    <script>

  var English12TOC = {
	Std: "12",
	Subject: "English",
	Chapters: [
		    "UNIT - 1: Prose TOPIC: Two Gentlemen of Verona",
        
        
       
      ],
    Mcqs: [
      English12Chapter1MCQ,
      
      ],
    Briefly: [
      English12Chapter1_5M,
      
      ]
    
};  
</script>
   
<script>   

    
$(document).ready(function(){
var i=0;

    // PrintTOC(Physics12TOC);
    
    //for(i=0; i< Physics12TOC.Mcqs.length; i++) {
        gCurChapter=0;
        PrintChapter(gCurChapter);
       
       
    //}

 $("#Next").click(function(){
    if( gCurChapter < English12TOC.Chapters.length-1) { 
       gCurChapter++;
       PrintChapter(gCurChapter);
        
       MathJax.Hub.Queue(["Typeset",MathJax.Hub]);

    }
  });
  $("#Prev").click(function(){
    if(gCurChapter>0) {
       gCurChapter--;
       PrintChapter(gCurChapter);
       MathJax.Hub.Queue(["Typeset",MathJax.Hub]);
    }
  });
    
});
</script>
    <script>
    function PrintChapter( i) {
         document.getElementById("mcq").innerHTML = "";
       PrintMCQ(i, English12TOC.Mcqs[i]); 
        document.getElementById("mcq").innerHTML += "";
       Print2Marks(English12TOC.Briefly[i]);
       document.getElementById("chap").innerHTML = English12TOC.Chapters[i];
    }
    function PrintTOC(toc) {
        var i=0;
        var x=0;
        var str = "";
        var obj = toc;
        
        document.getElementById("toc").innerHTML = "Class :" + obj.Std + ", " + obj.Subject +"<br>";
        for (i = 0; i < obj.Chapters.length; i++) {
            x=i+1;
            str = "<a href =\"index"+x+".html\">" + obj.Chapters[i] + "</a>"
            console.log(str);
            document.getElementById("toc").innerHTML += str + "<br>";
        }
        
    }
    
    </script>
    <script>
    function PrintMCQ( c, mcq) { 
        var i=0;
        var x="";
        var id = "";
        var str = "";
        //var button1 = "<button onclick="getElementById(id).innerHTML='C'">Ans</button>"
        
        
        for (i = 0; i < mcq.length; i++) {
            str = "<br><br><b>"+mcq[i].question + "</b><br>";
            if( typeof mcq[i].answers.figure != 'undefined')
               str += mcq[i].answers.figure + "<br>";
            str += "a. "+ mcq[i].answers.a + "<br>" + "b. "+ mcq[i].answers.b + "<br>" 
                 + "c. "+mcq[i].answers.c + "<br>" +"d. "+ mcq[i].answers.d + "<br>";
            id = "c"+c+"q"+i;
           // str += "<br> Ans :" + mcq[i].correctAnswer + "<br>";
           str += "<div id=" + "'" + id +"'> "+"</div>"; 

            x= "<button onclick=\"getElementById(" + "'"+id +"'" + ").innerHTML=" + "' Ans = " + mcq[i].correctAnswer + "'" +'"'+ "> Ans </button><br>";
            console.log(x);
            document.getElementById("mcq").innerHTML += str + x;
        }
    }
    function Print2Marks( qa ) {
       var i=0;
       var str = "";
    
       for(i=0; i< qa.length; i++) {
           str = "<br><br><b>"+qa[i].question + "</b><br>";
           str += qa[i].answer + "<br>";
            
            console.log(str);
            document.getElementById("mcq").innerHTML += str ;
       }
    
    }
    
    </script>
